{
  "name": "@yukihito/mysql-mcp-server",
  "version": "1.0.0",
  "description": "MySQL MCP Server",
  "type": "module",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "bin": {
    "mysql-mcp-server": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "mcp",
    "mysql",
    "server",
    "ai",
    "llm"
  ],
  "author": "Yukihito",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@10.22.0",
  "devDependencies": {
    "typescript": "^5.9.3"
  },
  "files": [
    "dist",
    "README.md"
  ]
}

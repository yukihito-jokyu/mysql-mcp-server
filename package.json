{
  "name": "@yukihito/mysql-mcp-server",
  "version": "1.0.1",
  "mcpName": "io.github.yukihito-jokyu/mysql-mcp-server",
  "description": "MySQL MCP Server",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "bin": {
    "mysql-mcp-server": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc && chmod 755 dist/index.js",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "prepublishOnly": "pnpm build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yukihito-jokyu/mysql-mcp-server.git"
  },
  "keywords": [
    "mcp",
    "mysql",
    "server",
    "modelcontextprotocol",
    "context"
  ],
  "author": "Yukihito",
  "license": "MIT",
  "type": "module",
  "packageManager": "pnpm@10.22.0",
  "files": [
    "dist",
    "README.md"
  ],
  "bugs": {
    "url": "https://github.com/yukihito-jokyu/mysql-mcp-server/issues"
  },
  "homepage": "https://github.com/yukihito-jokyu/mysql-mcp-server#readme",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.22.0",
    "commander": "^14.0.2",
    "mysql2": "^3.15.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "typescript": "^5.9.3"
  }
}
